{
  "name": "diveni-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve --port 8080",
    "serve:remote": "vue-cli-service serve --port 8080 --mode remote",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint --no-fix && npm run prettier -- --check",
    "fmt": "vue-cli-service lint && npm run prettier -- --write",
    "prettier": "prettier \"src/**/*.{vue,css,html,ts,js}\"",
    "licenses:update": "license-checker --json > ./public/frontend-dependencies.json"
  },
  "dependencies": {
