    "@types/sockjs-client": "^1.5.1",
    "axios": "^0.23.0",
    "bootstrap": "^4.6.1",
    "bootstrap-vue": "^2.22.0",
    "canvas-confetti": "^1.4.0",
    "core-js": "^3.6.5",
    "papaparse": "^5.3.1",
    "popper.js": "^1.16.1",
    "portal-vue": "^2.1.7",
    "qrcode.vue": "^1.7.0",
    "register-service-worker": "^1.7.1",
    "sockjs-client": "^1.5.2",
    "uuid": "^8.3.2",
    "vue": "^2.7.10",
    "vue-axios": "^3.3.7",
    "vue-i18n": "^8.26.8",
    "vue-router": "^3.2.0",
